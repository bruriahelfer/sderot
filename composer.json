{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/admin_toolbar": "^3.6",
        "drupal/all_in_one_accessibility": "^1.5",
        "drupal/antibot": "^2.0",
        "drupal/backup_migrate": "^5.1",
        "drupal/bartik": "1.1",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/block_content_permissions": "^1.11",
        "drupal/calendar": "1.0.0-beta4",
        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor_media_embed": "^2.0",
        "drupal/classy": "2.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/crop": "^2.5",
        "drupal/ctools": "^4.1",
        "drupal/date_popup": "^2.0",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/entityconnect": "^2.0@RC",
        "drupal/extlink": "^2.0",
        "drupal/field_group": "^4.0",
        "drupal/fullcalendar_view": "5.2",
        "drupal/gin": "4.1",
        "drupal/google_analytics": "^4.0",
        "drupal/gtm": "^1.10",
        "drupal/image_resize_filter": "^1.2",
        "drupal/image_widget_crop": "^3.0",
        "drupal/jquery_ui": "^1.8",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/linkit": "^7.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/menu_admin_per_menu": "^1.7",
        "drupal/menu_item_fields": "^1.11",
        "drupal/menu_link_attributes": "^1.5",
        "drupal/metatag": "^2.2",
        "drupal/native_lazy_loading": "^1.1@RC",
        "drupal/paragraphs": "^1.19",
        "drupal/paragraphs_browser": "^1.3",
        "drupal/paragraphs_edit": "^3.0",
        "drupal/pathauto": "^1.14",
        "drupal/quick_node_clone": "^1.22",
        "drupal/rdf": "^2.1",
        "drupal/redirect": "^1.12",
        "drupal/roleassign": "^2.0",
        "drupal/seven": "1.0",
        "drupal/similarterms": "^1.9",
        "drupal/simple_gmap": "4.0",
        "drupal/simple_sitemap": "^4.2",
        "drupal/svg_image": "3.2",
        "drupal/tablefield": "3.0",
        "drupal/token": "^1.16",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/video_embed_field": "^2.5",
        "drupal/view_unpublished": "^1.3",
        "drupal/views_ajax_history": "^1.8",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_templates": "^1.6",
        "drupal/webform": "^6.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
